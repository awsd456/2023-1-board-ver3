<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.boardver3.board.BoardMapper">


<insert id="insBoard" parameterType="BoardInsDto">
    insert into t_board
    (title,ctnt,iuser)
    values
    (#{title},#{ctnt},#{iuser})
</insert>
<select id="selBoard" resultType="BoardVo">
    select iboard,title,ctnt,iuser,created_at,updated_at from t_board
    limit ${stardIdx},${lowRen}
</select>



</mapper>